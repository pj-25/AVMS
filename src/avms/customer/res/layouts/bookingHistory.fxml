<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #434343;" xmlns="http://javafx.com/javafx/8.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="avms.customer.controller.BookingHistoryController">
    <children>
        <Label id="pageTitle" text="Booking History" textFill="#03cd7f">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
        <Separator prefWidth="200.0" />
        <ImageView fitHeight="300.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/ticket-machine.png" />
            </image>
        </ImageView>
        <TableView fx:id="bookingHistory" prefHeight="200.0" prefWidth="200.0" tableMenuButtonVisible="true">
            <placeholder>
                <Label text="No Booking History" />
            </placeholder>
            <columns>
                <TableColumn fx:id="column1" prefWidth="75.0" text="Booking ID" />
                <TableColumn fx:id="column2" prefWidth="75.0" text="Vehicle ID" />
                <TableColumn fx:id="column3" prefWidth="75.0" text="Parking Service ID" />
                <TableColumn fx:id="column4" prefWidth="75.0" text="Slot ID" />
                <TableColumn fx:id="column5" prefWidth="75.0" text="Starting Time" />
                <TableColumn fx:id="column6" prefWidth="75.0" text="Reserved Till" />
            </columns>
        </TableView>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />

    </padding>
</VBox>
