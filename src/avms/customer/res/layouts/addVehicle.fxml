<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #434343;" xmlns="http://javafx.com/javafx/8.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="avms.customer.controller.AddVehicleController">
     <children>
         <Label id="pageTitle" text="Add Vehicle" textFill="#03cd7f">
             <font>
                 <Font name="System Bold" size="30.0" />
             </font>
         </Label>
      <Separator prefWidth="200.0" />
         <ImageView fitHeight="300.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
             <image>
                 <Image url="@../images/insurance.png" />
             </image>
         </ImageView>
         <HBox alignment="TOP_CENTER" spacing="10.0">
             <children>
                 <Label text="Vehicle Type" />
                 <TextField fx:id="vehicleType" />
             </children>
         </HBox>
         <HBox alignment="TOP_CENTER" layoutX="10.0" layoutY="221.0" spacing="10.0">
             <children>
                 <Label text="Vehicle Number" />
            <TextField fx:id="vehicleNumber" />
             </children>
         </HBox>
      <HBox alignment="TOP_CENTER" layoutX="30.0" layoutY="422.0" spacing="10.0">
         <children>
            <Label text="Model Name" />
            <TextField fx:id="modelName" />
         </children>
      </HBox>
         <Button fx:id="submitBtn" mnemonicParsing="false" onAction="#submit" text="Submit" />
     </children>
     <padding>
         <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />

     </padding>
 </VBox>
