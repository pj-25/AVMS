<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox fx:id="viewPaneChild" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="avms.userProfile.controller.ProfileController">
   <children>
        <Label id="pageTitle" text="Profile" textFill="#03cd7f">
            <font>
                <Font name="System Bold" size="30.0" />
            </font>
        </Label>
      <Separator prefWidth="200.0" />
      <HBox alignment="CENTER" spacing="10.0">
         <children>
              <ImageView fitHeight="300.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                      <Image url="@../images/account.png" />
                  </image>
              </ImageView>
            <VBox prefHeight="371.0" prefWidth="456.0" spacing="10.0" style="-fx-background-color: #434343;">
                <children>
                    <HBox spacing="10.0">
                        <children>
                            <Label text="User Id:" />
                            <Label fx:id="userId" />
                        </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="236.0" spacing="10.0">
                        <children>
                            <Label text="First Name" />
                        <Label fx:id="firstName" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="236.0" spacing="10.0">
                        <children>
                            <Label text="Last Name" />
                        <Label fx:id="lastName" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="380.0" spacing="10.0">
                        <children>
                            <Label text="Age:" />
                        <Label fx:id="age" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="488.0" spacing="10.0">
                        <children>
                            <Label text="User Type:" />
                        <Label fx:id="userType" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="452.0" spacing="10.0">
                        <children>
                            <Label text="Address Line:" />
                        <Label fx:id="addressLine" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="524.0" spacing="10.0">
                        <children>
                            <Label text="City:" />
                        <Label fx:id="city" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="560.0" spacing="10.0">
                        <children>
                            <Label text="State:" />
                        <Label fx:id="state" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="596.0" spacing="10.0">
                        <children>
                            <Label text="Country:" />
                        <Label fx:id="country" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="632.0" spacing="10.0">
                        <children>
                            <Label text="ZipCode:" />
                        <Label fx:id="zipcode" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="632.0" spacing="10.0">
                        <children>
                            <Label text="Email ID:" />
                        <Label fx:id="emailId" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="668.0" spacing="10.0">
                        <children>
                            <Label text="Phone Number:" />
                        <Label fx:id="phoneNumber" layoutX="10.0" layoutY="10.0" />
            
                              </children>
                    </HBox>
                    <HBox layoutX="30.0" layoutY="740.0" spacing="10.0">
                        <children>
                            <Label text="Gender:" />
                        <Label fx:id="gender" layoutX="10.0" layoutY="10.0" />
                        </children>
                    </HBox>
                </children>
                <padding>
                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                </padding>
            </VBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
